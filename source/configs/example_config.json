{
    "exp_name": "example",
    "data": {
        "audio": {
            "rate_sample": 48000,
            "spec": {
                "nb_freqs": 1024,
                "size_win": 1024,
                "stride_win": 256,
                "mel": {
                    "nb_mel_freqs": 64,
                    "freq_min": 20,
                    "freq_max": 20000
                }
            }
        },
        "annotations": {
            "size_kernel": 3,
            "sigma": 1
        }
