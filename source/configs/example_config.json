    "model": {
        "conv": {
            "size_kernel": [
                3,
                3
            ],
            "rate_dilation": 3
        },
        "ae": {
            "size_latent": 1024
        }
    },
    "training": {
        "rate_learning": 0.02,
        "size_batch": 8,
        "nb_epochs": 10
    },
    "dataset": {
        "name": "canonne_duos",
        "validation_split": 0.1
    },
    "save": {
        "log": {
            "update_freq": 1
        },
        "checkpoint": {
            "max_to_keep": 5,
            "checkpoint_interval": 2
        }
    },
    "debug": {
        "enabled": true
    }
}